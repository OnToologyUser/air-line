#################################################################
#    Classes
#################################################################

###  http://dbpedia.org/ontology/Aircraft
<http://dbpedia.org/ontology/Aircraft> rdf:type owl:Class ;
                                       rdfs:subClassOf owl:Thing ;
                                       rdfs:comment "An aircraft is a vehicle that is able to fly by gaining support from the air. It counters the force of gravity by using either static lift or by using the dynamic lift of an airfoil, or in a few cases the downward thrust from jet engines. Common examples of aircraft include airplanes, helicopters, airships (including blimps), gliders, paramotors, and hot air balloons."@en ;
                                       rdfs:isDefinedBy "https://dbpedia.org/ontology/"@en ;
                                       rdfs:label "Aircraft"@en .


###  http://dbpedia.org/ontology/Airline
<http://dbpedia.org/ontology/Airline> rdf:type owl:Class ;
                                      rdfs:subClassOf owl:Thing ;
                                      rdfs:comment "An airline is a company that provides air transport services for traveling passengers and freight. Airlines use aircraft to supply these services and may form partnerships or alliances with other airlines for codeshare agreements, in which they both offer and operate the same flight. Generally, airline companies are recognized with an air operating certificate or license issued by a governmental aviation body. Airlines may be scheduled or charter operators."@en ;
                                      rdfs:isDefinedBy "https://dbpedia.org/ontology/"@en ;
                                      rdfs:label "Airline"@en .


###  http://dbpedia.org/ontology/Country
<http://dbpedia.org/ontology/Country> rdf:type owl:Class ;
                                      rdfs:subClassOf :Passenger ;
                                      rdfs:comment "A country is a distinct part of the world, such as a state, nation, or other political entity. It may be a sovereign state or make up one part of a larger state. For example, the country of Japan is an independent, sovereign state, while the country of Wales is a component of a multi-part sovereign state, the United Kingdom. A country may be a historically sovereign area (such as Korea), a currently sovereign territory with a unified government (such as Senegal), or a non-sovereign geographic region associated with certain distinct political, ethnic, or cultural characteristics (such as the Basque Country)."@en ;
                                      rdfs:isDefinedBy "https://dbpedia.org/ontology/"@en ;
                                      rdfs:label "Country"@en .


###  https://w3id.org/def/airlinereviews#Aircraft
:Aircraft rdf:type owl:Class ;
          rdfs:subClassOf <http://dbpedia.org/ontology/Aircraft> ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasQatarAirline ;
                            owl:allValuesFrom :QatarAirways
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :hasRyanairAirline ;
                            owl:allValuesFrom :Ryanair
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :id ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:anyURI
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :name ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :seatComfort ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onDataRange xsd:float
                          ] ;
          rdfs:comment "Machine that is able to fly by gaining support from the air other than the reactions of the air against the earth’s surface."@en ;
          rdfs:isDefinedBy "https://w3id.org/def/airlinereviews#"@en ;
          rdfs:label "Aircraft"@en .


###  https://w3id.org/def/airlinereviews#Airline
:Airline rdf:type owl:Class ;
         rdfs:subClassOf <http://dbpedia.org/ontology/Airline> ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :id ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:anyURI
                         ] ,
                         [ rdf:type owl:Restriction ;
                           owl:onProperty :name ;
                           owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                           owl:onDataRange xsd:string
                         ] ;
         rdfs:comment "Airlines are the entities that own the planes that use your airport."@en ;
         rdfs:isDefinedBy "https://w3id.org/def/airlinereviews#"@en ;
         rdfs:label "Airline"@en .


###  https://w3id.org/def/airlinereviews#Author
:Author rdf:type owl:Class ;
        rdfs:subClassOf :Review ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasBusinessClass ;
                          owl:allValuesFrom :BusinessClass
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasEconomyClass ;
                          owl:allValuesFrom :EconomyClass
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :hasFirstClass ;
                          owl:allValuesFrom :FirstClass
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :isPassenger ;
                          owl:allValuesFrom :Passenger
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :id ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:anyURI
                        ] ,
                        [ rdf:type owl:Restriction ;
                          owl:onProperty :name ;
                          owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                          owl:onDataRange xsd:string
                        ] ;
        rdfs:comment "the 'author' of an airline review is usually the individual passenger who wrote the review."@en ;
        rdfs:isDefinedBy "https://w3id.org/def/airlinereviews#"@en ;
        rdfs:label "Author"@en .


###  https://w3id.org/def/airlinereviews#Business
:Business rdf:type owl:Class ;
          rdfs:subClassOf :PassengerType ;
          rdfs:comment "Business Passenger Type."@en ;
          rdfs:isDefinedBy "https://w3id.org/def/airlinereviews#"@en ;
          rdfs:label "Business"@en .


###  https://w3id.org/def/airlinereviews#BusinessClass
:BusinessClass rdf:type owl:Class ;
